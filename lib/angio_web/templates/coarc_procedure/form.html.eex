</div><!----column-------------------->
 <div class = "column">
<div class="field">
            <label class="label field_help " > Post-Procedure Total Ascending to Descending Aortic Systolic Gradient</label>
            <%= text_input f, :coarc_ao_obstruction_post_sys_gradient,
            class: "input", type: "text",
            placeholder: "coarc_ao_obstruction_post_sys_gradient",
            required: false %>
            <%= error_tag f, :coarc_ao_obstruction_post_sys_gradient %>
          <p class="help"></p>
        </div>


 </div><!----column-------------------->

</div><!----columns--------------------------->

<!---------end start------------------------------------>





</div><!----------LEFT COLUMN------------------------>

<div class="column frame_1">


<!---------start----------------------------------->
  <div class = "columns">
<div class = "column">

  <div class="field">
  <label class="label"> My Notes On Coarctation Procedure</label>
  <div class="control">
    <%= text_input f, :coarc_note,
        class: "textarea",
        type: "input", placeholder: "coarc_note"  %>
  <p class="help"></p>

  </div><!---control---------->
   </div><!-------field-------->
</div><!----column-------------------->
