        </div>


</div><!----column-------------------->
</div><!----columns--------------------------->

<!---------end start------------------------------------>




<!---------start----------------------------------->
  <div class = "columns">

<div class = "column">

<!---------field------------------------->
 <div class="field">
        <label class="label"> Coarctation with Additional Associated Aortic Obstruction</label>
        <div class="control">
         <span class="field_help"></span>
          <%= checkbox f, :coarc_aortic_obstruction_yn,
            type: "checkbox" %>
            <%= error_tag f,
           :coarc_aortic_obstruction_yn %>
        </div>
        <p class="help">Check if Yes
        </p>
    </div><!-----field------------------>



</div><!----column-------------------->
 <div class = "column">

<!---------field------------------------->
 <div class="field">
        <label class="label"> Additional Intervention on Aortic Arch?</label>
        <div class="control">
         <span class="field_help"></span>
          <%= checkbox f, :coarc_arch_treated_yn,
            type: "checkbox" %>
            <%= error_tag f,
           :coarc_arch_treated_yn %>
        </div>
        <p class="help">Check if Yes
        </p>
    </div><!-----field------------------>

 </div><!----column-------------------->

</div><!----columns--------------------------->

<!---------end start------------------------------------>



<!---------start----------------------------------->
  <div class = "columns">

<div class = "column">

<div class="field">
            <label class="label field_help " >Pre-Procedure Total Ascending to Descending Ao Sys Gradient </label>
            <%= text_input f, :coarc_ao_obstruction_pre_sys_gradient,
