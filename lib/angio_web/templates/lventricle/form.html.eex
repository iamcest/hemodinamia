      <div class="columns important_column ">
 <div class="column ">


 <div class="field">
          <label class="label"> LV Is Normal?</label>
          <div class="control">
            <%= checkbox f, :cath_lv_normal_yn  ,
             placeholder: "cath_lv_normal_yn",
            required: false,
type: "checkbox" %>
<%= error_tag f, :cath_lv_normal_yn %>
           <p class = "help">
             Check only If LV is Normal
           </p>

          </div>
           </div>
        <!--field---->

 </div><!--------column------------>
  </div><!----columns------------->

  <!--------end start----------------------->

<span class="button is-primary is-medium is-fullwidth ">
    General
 </span>

