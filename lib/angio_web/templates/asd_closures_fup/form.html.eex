
<% alias Angio.Canons, as: Query %>

<%= form_for @changeset, @action, fn f -> %>
  <%= if @changeset.action do %>
  <div class="notification is-danger">
    <button class="delete"></button>
    Oops, something went wrong! Please check the errors below.
</div>

  <% end %>

<div class="columns">
<div class="column frame_1">

<!---------start----------------------------------->
  <div class = "columns">
<div class = "column">


<!--------start---field----------------------------->
 <div class="field">
          <label class="label">Follow-up Date  </label>
          <div class="control">

      <%= text_input f, :asdclf_assessment_dt,
class: "input",
type: "date",
placeholder: "asdclf_assessment_dt",
required: true %>
<%= error_tag f,:asdclf_assessment_dt %>
          </div>
          <p class="help"></p>
        </div>
        <!---------end ---field------------>


</div><!----column-------------------->

 <div class = "column">



<!--------start---field----------------------------->
 <div class="field">
  <label class="label">ASD Closure Date  </label>
