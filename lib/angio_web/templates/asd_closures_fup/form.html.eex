<div class = "column">


<div class="field">
          <label class="label">Is Free Of Events?</label>
          <div class="control">
            <%= checkbox f, :asdclf_free_events_yn,
             placeholder: "caepsds_zip_na",
            required: false,
            type: "checkbox" %> <%= error_tag f,
            :asdclf_free_events_yn %>
          </div>
          <p class="help">Check if Yes</p>
        </div>
        <!--------------------end------field------------------------->

</div><!----column-------------------->

 <div class = "column">

<!-------start Field------------------------------------>
